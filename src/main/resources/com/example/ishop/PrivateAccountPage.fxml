<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.ishop.controller.page_controllers.PrivateAccountPageController">
    <Label fx:id="idInfo" layoutX="286.0" layoutY="56.0" text="id"/>
    <Label fx:id="usernameInfo" layoutX="286.0" layoutY="86.0" text="username"/>
    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true"/>
    <Label fx:id="passwordInfo" layoutX="286.0" layoutY="119.0" text="password"/>
    <Label fx:id="emailInfo" layoutX="286.0" layoutY="150.0" text="email"/>
    <Button fx:id="goToOrdersPage" layoutX="497.0" layoutY="40.0" mnemonicParsing="false" text="My orders"/>
    <Button fx:id="goToCartPage" layoutX="497.0" layoutY="82.0" mnemonicParsing="false" text="My cart"/>
    <Button fx:id="logOut" layoutX="495.0" layoutY="124.0" mnemonicParsing="false" onAction="#logOutCustomer"
            text="Log Out"/>
    <Button fx:id="goToMainPage" layoutX="495.0" layoutY="175.0" mnemonicParsing="false" onAction="#goToMainPage"
            text="Go to main page"/>
    <Label layoutX="228.0" layoutY="57.0" text="ID"/>
    <Label layoutX="228.0" layoutY="86.0" text="Username"/>
    <Label layoutX="228.0" layoutY="119.0" text="Password"/>
    <Label layoutX="228.0" layoutY="150.0" text="Email"/>
</AnchorPane>
