<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.ishop.controller.page_controllers.LogInPageController">

    <Button fx:id="goToMainScreenButton" layoutX="500.0" layoutY="300.0" mnemonicParsing="false"
            onAction="#goToMainScreen" text="GoToMainScreen"/>
    <Button fx:id="goToCreateAccountPageButton" layoutX="1.0" layoutY="300.0" mnemonicParsing="false"
            onAction="#goToCreateAccountPage" text="goToCreateAccountPage"/>
    <Button fx:id="logInButton" layoutX="396.0" layoutY="152.0" mnemonicParsing="false" onAction="#logIn"
            text="Log IN"/>
    <Button fx:id="logInAsVendor" layoutX="341.0" layoutY="200.0" mnemonicParsing="false" onAction="#goToVendorPage"
            text="Log IN ad Vendor"/>
    <TextField fx:id="usernameField" layoutX="159.0" layoutY="58.0" prefHeight="25.0" prefWidth="289.0"
               onKeyPressed="#keyboardListener"/>
    <TextField fx:id="passwordField" layoutX="159.0" layoutY="101.0" prefHeight="25.0" prefWidth="289.0"
               onKeyPressed="#keyboardListener"/>
    <Label layoutX="96.0" layoutY="62.0" text="Username"/>
    <Label layoutX="96.0" layoutY="105.0" text="Password"/>
    <Label fx:id="infoMessageLabel" layoutX="218.0" layoutY="135.0" prefHeight="17.0" prefWidth="172.0"/>
    <Label fx:id="infoMessageLabel2" layoutX="218.0" layoutY="83.0" prefHeight="17.0" prefWidth="172.0"/>
</AnchorPane>
