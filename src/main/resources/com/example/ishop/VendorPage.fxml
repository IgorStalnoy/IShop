<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.ishop.controller.page_controllers.VendorPageController">

    <Label fx:id="infoMessage" layoutX="175.0" layoutY="235.0" prefHeight="17.0" prefWidth="250.0" visible="false"/>
    <Label fx:id="vendorPasswordInfoMessage" layoutX="20.0" layoutY="104.0" prefHeight="17.0" prefWidth="228.0"/>
    <TextField fx:id="vendorPasswordField" layoutX="20.0" layoutY="62.0" onKeyPressed="#vendorPasswordInput"/>
    <Button fx:id="fileOpenButton" layoutX="396.0" layoutY="152.0" mnemonicParsing="false" onAction="#loadFile"
            text="Load file" visible="false"/>
    <Button fx:id="goToMainPage" layoutX="500.0" layoutY="152.0" mnemonicParsing="false" onAction="#goToMainPage"
            text="goToMainPage" visible="false"/>
    <Button fx:id="goToLogInPage" layoutX="500.0" layoutY="300.0" mnemonicParsing="false" onAction="#goToLogInPage"
            text="goToLogInPage"/>
</AnchorPane>
